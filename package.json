    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "zod": "^3.21.4",
    "zod-express-middleware": "^1.4.0"
  }
